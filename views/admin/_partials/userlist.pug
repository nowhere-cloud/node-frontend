thead
  tr
    th #
    th Username
    th Action
tbody
  if data.length
    each value, index in data
      tr
        td= value.id
        td= value.username
        td.clearfix
          .btn-group.float-right.ml-0.mr-0
            button.btn.btn-sm.btn-primary.dns-edit(data-entryid=`${value.id}`) Reset Password
            a.btn.btn-sm.btn-danger.dns-delete(href=`/admin/users/delete/${value.id}`) Delete User
  else
    tr.table-info
      td(colspan="3") No Records
