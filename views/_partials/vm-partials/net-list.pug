table.table
  thead
    tr
      th #
      th Network UUID
      th &nbsp;
  tbody
    if data.Status === 'Success'
      if data.Value.length > 0
        each value, index in data.Value
          tr
            td= index
            td.overflow= value
            td.clearfix
              a.btn.btn-sm.btn-primary.get-detail(href=`/admin/instances/net/${value}`) Get Detail
      else
        tr.table-info
          td(colspan="3") No Records.
    else
      tr.table-danger
        td(colspan="3")= data.ErrorDescription[0]
